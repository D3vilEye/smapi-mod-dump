<!-- stylecop.json: -->
<!-- https://github.com/DotNetAnalyzers/StyleCopAnalyzers/blob/master/documentation/Configuration.md -->

<RuleSet Name="RuleSet" ToolsVersion="10.0">
	<Rules AnalyzerId="StyleCop.Analyzers" RuleNamespace="StyleCop.Analyzers">
		<Rule Id="SA0001" Action="None" /> <!-- XmlCommentAnalysisDisabled -->
		<Rule Id="SA1025" Action="None" /> <!-- CodeMustNotContainMultipleWhitespaceInARow -->
		<Rule Id="SA1501" Action="None" /> <!-- StatementMustNotBeOnSingleLine -->
		<Rule Id="SA1503" Action="None" /> <!-- BracesMustNotBeOmitted -->
		<Rule Id="SA1623" Action="None" /> <!-- PropertySummaryDocumentationMustMatchAccessors -->
		<Rule Id="SA1633" Action="None" /> <!-- FileMustHaveHeader -->
	</Rules>
</RuleSet>
