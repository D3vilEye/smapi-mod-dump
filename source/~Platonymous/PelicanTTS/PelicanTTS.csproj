<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>PelicanTTS</AssemblyName>
    <RootNamespace>PelicanTTS</RootNamespace>
    <Version>1.8.4</Version>
    <TargetFramework>net452</TargetFramework>
    <Platforms>x86</Platforms>
    <PlatformTarget>x86</PlatformTarget>

    <EnableHarmony>true</EnableHarmony>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.Core" Version="3.3.106.1" />
    <PackageReference Include="AWSSDK.Polly" Version="3.3.103.68" />
    <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="3.1.0" />
    <PackageReference Include="Platonymous.Ogg2XNA" Version="1.0.0" />
  </ItemGroup>

  <Import Project="$(SolutionDir)\common.targets" />

</Project>
